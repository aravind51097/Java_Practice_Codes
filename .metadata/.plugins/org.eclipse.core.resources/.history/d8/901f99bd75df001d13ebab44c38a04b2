package com.java.practice;


class Sum{
	
	void sumOfGivenDigits(int num) {
		
		if(num<=10) {
			//return num;
		}
		else {
			int temp=0;
			for(int i = 0 ;i<=num;i++) {
				
				int digit=num%10;
				System.out.println(digit);
				
			}
			
		}
		
	}
}

public class SumOfAllDigits {
	public static void main(String[] args) {
		
		Sum obj=new Sum();
		
		obj.sumOfGivenDigits(55);
		
	}

}
