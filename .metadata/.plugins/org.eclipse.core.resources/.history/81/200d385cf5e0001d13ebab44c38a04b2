package com.java.practice;

class Flags{
	
	void americanFlag() {
		
		for(int row=0;row<16;row++) {
			for(int col=0;col<=36;col++) {
				if(row<10) {
					if(col<=5) {
						if(row%2==0) {
							System.out.print("* ");
					
							}
							else  {
								System.out.print(" *");
							}
							}else if (row%2==0){
								System.out.print("=");
							}else if(row%2!=0){
								System.out.print("=");
							}
							}
						else {
					
							System.out.print("=");
						}
			}
			System.out.println();
			
		}
	}
}


public class PrintingAmericanFalg {
	public static void main(String[] args) {
		
		Flags obj=new Flags();
		obj.americanFlag();
		
	}

}
